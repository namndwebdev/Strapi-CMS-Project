{
  "connection": "default",
  "collectionName": "post",
  "info": {
    "name": "post",
    "description": ""
  },
  "options": {
    "timestamps": [
      "createdAt",
      "updatedAt"
    ]
  },
  "attributes": {
    "topic": {
      "required": true,
      "type": "string"
    },
    "title": {
      "required": true,
      "type": "string"
    },
    "content": {
      "required": true,
      "type": "richtext"
    },
    "comments": {
      "collection": "comment"
    },
    "user": {
      "model": "user",
      "via": "posts",
      "plugin": "users-permissions"
    }
  }
}