{
  "connection": "default",
  "collectionName": "account",
  "info": {
    "name": "account",
    "description": ""
  },
  "options": {
    "timestamps": [
      "createdAt",
      "updatedAt"
    ]
  },
  "attributes": {
    "username": {
      "type": "string",
      "unique": true,
      "required": true
    },
    "password": {
      "minLength": 8,
      "required": true,
      "type": "password"
    },
    "avatar": {
      "type": "string"
    },
    "email": {
      "unique": true,
      "required": true,
      "type": "email"
    },
    "comments": {
      "collection": "comment",
      "via": "account"
    },
    "posts": {
      "collection": "post",
      "via": "account"
    },
    "Status": {
      "default": "active",
      "type": "string",
      "required": true,
      "unique": true
    },
    "Type": {
      "required": true,
      "default": "2",
      "type": "string"
    }
  }
}